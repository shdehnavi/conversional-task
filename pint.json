{
    "preset": "laravel",
    "rules": {
        "blank_line_before_statement": true,
        "concat_space": {
            "spacing": "one"
        },
        "method_argument_space": {
            "on_multiline": "ensure_fully_multiline"
        },
        "single_trait_insert_per_statement": true,
        "types_spaces": {
            "space": "single"
        }
    },
    "exclude": [
        "docker/data"
    ]
}
